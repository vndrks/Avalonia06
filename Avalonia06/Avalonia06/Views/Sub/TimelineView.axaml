<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:vms="clr-namespace:Avalonia06.ViewModels.Sub;assembly=Avalonia06"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Avalonia06.Views.Sub.TimelineView"
			 x:DataType="vms:TimelineViewModel" x:CompileBindings="True" Background="Gray">
	
	<StackPanel Background="Gray">
		<Image Source="{Binding ImageFromBinding}" Stretch="UniformToFill" />
	</StackPanel>

</UserControl>
